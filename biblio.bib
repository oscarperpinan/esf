}

@InProceedings{Abete.Scapino.ea2001,
  Title                    = {Comparison of Power Quality between Centralised Invertens and Module Integrated Inverters in Grid Connected {PV} Systems},
  Author                   = {Abete, A. and Scapino, F. and Spertino, F. and Conference, European Photovoltaic Solar Energy},
  Year                     = {2001},

  Abstract                 = {The grid connected PV systems can have a centralised inverter for the whole array of PV modules or as many inverters as the PV modules are (module integrated inverters). The paper presents a comparison of power quality between centralised inverters and module integrated inverters. The comparison is based on normalised experimental tests, in order to determine efficiency, power factor, harmonic distortion and islanding time. From many tests a module integrated inverter provides the best power quality, showing that module integrated inverters are a suitable solution for the grid connected PV systems},
  File                     = {Abete.Scapino.ea2001.pdf:Abete.Scapino.ea2001.pdf:PDF},
