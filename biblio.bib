@Article{Chong.Wong2009,
  Title                    = {General formula for on-axis sun-tracking system and its application in improving tracking accuracy of solar collector},
  Author                   = {Chong, K. K. and Wong, C. W.},
  Journal                  = {Solar Energy},
  Year                     = {2009},
  Pages                    = {298--305},
  Volume                   = {83},

