@Unpublished{Vant-Hull,
  Title                    = {Layout of optimized heliostat fields},
  Author                   = {Vant-Hull, L. L.},

  Abstract                 = {Following our early suggestion, numerous central receiver design codes have employed the radial-stagger (RS) configuration to automatically produce a map of heliostat locations. Few of these maps show sufficient attention to detail to well represent the optimized design. The University of Houston RCELL optimization process provides for each cell a specification of optimized energy/m2 of reflector, as well as radial spacing and azimuthal spacing. Several issues are involved in properly implementing the RS configuration to provide heliostat locations. Conflicts in the azimuthal spacing are handled by adjusting the radial spacing to maintain the optimal energy/m2 of reflector. Moving inward, extreme azimuthal crowding occurs. This is alleviated by the introduction of zone boundaries where the angular azimuthal spacing is increased by a factor such as 5/4. The 5th heliostat in the last circle is deleted to alleviate severe blocking. Then, nearby heliostats are âslippedâ to take advantage of the âholeâ. The Solar One field exemplifies this procedure, and was 97% as effective as an âeverywhere optimalâ field.},
  File                     = {Vant-HullNA.pdf:Vant-HullNA.pdf:PDF},
  Keywords                 = {filed layout, heliostat, optimization, shading, tracking},
  Owner                    = {oscar},
  Refid                    = {Vant-Hull},
