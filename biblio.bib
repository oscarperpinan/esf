@Article{Whitcher.Guttorp.ea2000,
  Title                    = {Wavelet analysis of covariance with application to atmospheric time series},
  Author                   = {Whitcher, B. and Guttorp, P. and Percival, D. B.},
  Journal                  = {Journal of Geophysical Research-atmospheres},
  Year                     = {2000},

