@Article{Santamaria.Pantaleon.ea2000,
  Title                    = {A comparative study of high-accuracy frequency estimation methods},
  Author                   = {Ignacio Santamaría and Carlos Pantaleón and Jesús Ibañez},
  Journal                  = {Mechanical Systems and Signal Processing},
